<section class="home-list">

    #foreach ($id in [0..0])
        <div class="home-list-item">
            <div class="list-item-share">
                <i class="fi-share"></i>
            </div>
            <div class="list-item-user">
                <img src="${imageUrlPrefix}/images/logo/3.jpg" alt=""/>
                <span class="item-user-name">$!{questionList.get($id).userInfo.username}</span>
                #if ("$!{questionList.get($id).userInfo.userDesc}" != "")
                <span class="item-user-desc">，$!{questionList.get($id).userInfo.userDesc}</span>
                #end
            </div>
            <div class="list-item-title">
                <a href="/q?qid=$!{questionList.get($id).showId}">
                    <span class="item-type-tag">问答</span>
                    $!{questionList.get($id).question.title}
                </a>
            </div>
            <div class="list-item-preview">
                <a href="/q?qid=$!{questionList.get($id).showId}">
                    $!{questionList.get($id).question.description}
                </a>
            </div>
            <div class="list-item-sub">
                <span class="item-view-cnt">$!{questionList.get($id).question.viewCnt}浏览</span>
                &nbsp ∙ &nbsp
                <span class="item-up-cnt">$!{questionList.get($id).question.subCnt}赞同</span>
            </div>
        </div>
    #end

    <div class="home-list-item">
        <div class="list-item-share">
